<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.lrot.pingo">

<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@drawable/pospec_app_icon_no_border"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">

    <activity
        android:name="com.lrot.pingo.MainActivity"
        android:screenOrientation="sensorPortrait"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>

    <activity
        android:name="com.lrot.pingo.Bingo"
        android:screenOrientation="sensorPortrait"
        android:label=""
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar"></activity>

    <activity
        android:name="com.lrot.pingo.Info"
        android:screenOrientation="sensorPortrait"
        android:label=""
        android:theme="@style/Theme.AppCompat.NoActionBar"></activity>

    <activity
        android:name="com.lrot.pingo.Settings"
        android:screenOrientation="sensorPortrait"
        android:label=""
        android:theme="@style/Theme.AppCompat.NoActionBar"></activity>

</application>
</manifest>
